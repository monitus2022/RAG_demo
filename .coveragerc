[run]
source = src
omit =
    */tests/*
    */test_*
    */venv/*
    */__pycache__/*
    */.pytest_cache/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov